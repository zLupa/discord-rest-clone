    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.0",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^3.12.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.38",
